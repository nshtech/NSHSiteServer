<body class="sticky-header page-header">
    
    
    
    <noscript>
        <div class="javascript-required">
            <i class="fa fa-times-circle"></i> You seem to have Javascript disabled. This website needs javascript in order to function properly!
        </div>
    </noscript>
        

    <div id="wrap">
        
        {{> header}}
        <div id="content">
        
        <!-- /// CONTENT  /////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
            
            <div class="row-container contact">    
                <div class="row">
                    <div class="span4">
                        <h1 class="text-uppercase"><strong>Contact us</strong></h1>
                        <h3>Feel free to contact us regarding any inquiries, concerns, or comments about NSH. We will respond within a few business days.</h3><br>
                        <h3><strong>Address:</strong><br>
                        Innovation and New Ventures Office<br>
                        1800 Sherman Avenue, Suite 504<br>
                        Evanston, IL 60201</h3>

                    </div>
                    <div class="span8">
                        
                        
                        
                        <form class="fixed" name="contact-form" method="post" action='/contact'>  
                        <fieldset>
                            <div id="formstatus"></div>
                                    
                            <p>
                                <input class="span12" type="text" id="name" name="name" value="" placeholder="Name">
                            </p>
                            
                            <p>
                                <input class="span12" type="text" id="email" name="email" value="" placeholder="Email">
                            </p>
                            
                            <p>
                                <input class="span12" type="text" id="subject" name="subject" value="" placeholder="Subject">
                            </p>
                            
                            <p>
                                <textarea class="span12" id="message" name="message" rows="5" cols="25" placeholder="Message"></textarea>
                            </p>
                            <p class="last text-right">
                                <input class="btn text-uppercase" id="submit" type="submit" name="submit" value="Send" />
                            </p>
                        </fieldset>
                    </form><!-- end #contact-form -->
                        
                    </div><!-- end .span12 -->
                </div><!-- end .row -->
                <div class="row">
                    <div class="span12np">
                        
                        <div class="google-map map" 
                            data-zoom="17" 
                            data-address="1800 Sherman Avenue Evanston, IL 60201" 
                            data-caption="Innovation and New Ventures Office" >
                            <p>This will be replaced with the Google Map.</p>
                        </div><!-- end .google-map -->
                        
                    </div><!-- end .span12 -->
                </div><!-- end .row --> 
            </div>  <!-- end .row-content -->

                                                                
            
        <!-- //////////////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->

        </div><!-- end #content -->     
        {{> footer}}
        
    </div><!-- end #wrap -->
    
    <a id="back-to-top" href="#">
        <i class="ifc-up4"></i>
    </a>
    

</body>
</html>
